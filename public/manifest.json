{
  "$schema": "https://json.schemastore.org/chrome-manifest.json",
  "manifest_version": 3,
  "name": "Vite Extension Starter",
  "description": "Browser extension starter powered by Vite + React + Typescript",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "32": "vite.png"
    }
  },
  "permissions": [
    "scripting"
  ]
}